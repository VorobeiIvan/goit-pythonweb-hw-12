# SECRET_KEY is used for cryptographic operations, such as signing tokens.
SECRET_KEY=test_secret_key

# ACCESS_TOKEN_EXPIRE_MINUTES defines the expiration time for access tokens in minutes.
ACCESS_TOKEN_EXPIRE_MINUTES=30

# DATABASE_URL specifies the database connection string. Here, it uses an in-memory SQLite database for testing purposes.
DATABASE_URL=sqlite:///:memory:

# REDIS_HOST specifies the hostname or IP address of the Redis server.
REDIS_HOST=localhost

# REDIS_PORT defines the port number on which the Redis server is running.
REDIS_PORT=6379

# REDIS_PASSWORD is the password used to authenticate with the Redis server.
REDIS_PASSWORD=test_redis_password